{
  "name": "reisraff/phulp",
  "description": "The task manager for PHP",
  "type": "library",
  "version": "2.1.0",
  "keywords": [
    "phulp",
    "php",
    "gulp",
    "task",
    "task-manager",
    "task-runner",
    "builder",
    "assets-management",
    "watcher"
  ],
  "license": "MIT",
  "homepage": "https://reisraff.github.io/phulp/#!/home",
  "authors": [
    {
      "name": "Rafael Reis",
      "email": "reisraff@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/reisraff/phulp/issues",
    "wiki": "https://github.com/reisraff/phulp/wiki",
    "source": "https://github.com/reisraff/phulp"
  },
  "require": {
    "php": ">=5.6",
    "react/event-loop": "~0.4",
    "react/child-process": "^0.4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.4"
  },
  "autoload": {
    "psr-4": {
      "Phulp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phulp\\Test\\": "test"
    }
  },
  "config": {
    "platform": {
      "php": "5.6"
    },
    "bin-dir": "bin"
  },
  "bin": [
    "bin/phulp"
  ]
}
